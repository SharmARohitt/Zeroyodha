{
  "expo": {
    "name": "Wealth Warrior",
    "slug": "wealth-warrior",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Wealth.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/Wealth.png",
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
