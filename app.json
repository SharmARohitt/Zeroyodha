      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Wealth.png",
        "backgroundColor": "#000000"
      },
      "package": "com.wealthwarrior.app",
      "versionCode": 1
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "wealth-warrior-app"
      }
    },
    "scheme": "wealth-warrior",
    "owner": "your-expo-username"
